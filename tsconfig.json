{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "lib": [
            "es2015"
        ],
        "typeRoots": [
            "../node_modules/@types"
        ],
        "preserveConstEnums": true,
        "removeComments": true,
        "declaration": true,
        "noEmitOnError": true
    }
}