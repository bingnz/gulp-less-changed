{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "typeRoots": [
            "../node_modules/@types"
        ],
        "types": [
            "core-js"
        ],
        "preserveConstEnums": true,
        "removeComments": true,
        "declaration": true,
        "noEmitOnError": true
    }
}