{
  "name": "gulp-less-changed",
  "version": "0.0.0-semantically-released",
  "description": "Only pass .less files through the gulp stream if they or their dependencies have changed",
  "main": "release/main.js",
  "scripts": {
    "build": "gulp",
    "test": "niv less@2.7.2 && niv less@3.0.4 && nyc mocha && node generate-coverage-reports",
    "clean": "gulp clean",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bingnz/gulp-less-changed.git"
  },
  "keywords": [
    "gulpplugin",
    "less",
    "changed",
    "dependencies"
  ],
  "author": "David Chandler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bingnz/gulp-less-changed/issues"
  },
  "homepage": "https://github.com/bingnz/gulp-less-changed#readme",
  "devDependencies": {
    "@types/bluebird-global": "^3.5.9",
    "@types/less": "^3.0.0",
    "@types/mkdirp": "^0.5.2",
    "@types/object-assign": "^4.0.30",
    "@types/plugin-error": "^0.1.0",
    "@types/replace-ext": "0.0.27",
    "@types/through2": "^2.0.33",
    "@types/vinyl": "^2.0.2",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "del": "^3.0.0",
    "eslint": "^5.5.0",
    "fake-fs": "^0.5.0",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.0.1",
    "gulp-eslint": "^5.0.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-replace": "^1.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^5.0.0-alpha.3",
    "istanbul": "^0.4.5",
    "lazypipe": "^1.0.1",
    "less": "^3.8.1",
    "merge2": "^1.2.2",
    "mocha": "^5.2.0",
    "npm-install-version": "^6.0.2",
    "nyc": "^13.0.1",
    "pmock": "^0.2.3",
    "proxyquire": "^2.1.0",
    "remap-istanbul": "^0.13.0",
    "semantic-release": "^15.9.12",
    "sinon": "^6.1.5",
    "sinon-chai": "^3.2.0",
    "sinon-test": "^2.2.1",
    "stream-assert": "^2.0.3",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "mkdirp": "^0.5.1",
    "object-assign": "^4.1.1",
    "plugin-error": "^1.0.1",
    "replace-ext": "^1.0.0",
    "stream-to-array": "^2.3.0",
    "through2": "^3.0.0",
    "vinyl": "^2.2.0"
  },
  "peerDependencies": {
    "less": ">= 2.6.0 < 4.0.0"
  },
  "nyc": {
    "check-coverage": true,
    "extension": [
      ".js"
    ],
    "include": [
      "release/**/*.js"
    ],
    "require": [
      "babel-core/register",
      "babel-polyfill"
    ],
    "reporter": [
      "json"
    ],
    "sourceMap": true,
    "all": true,
    "report-dir": "coverage"
  }
}
