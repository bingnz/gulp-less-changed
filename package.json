{
  "name": "gulp-less-changed",
  "version": "0.0.0-semantically-released",
  "description": "Only pass .less files through the gulp stream if they or their dependencies have changed",
  "main": "release/main.js",
  "scripts": {
    "build": "gulp",
    "test": "nyc mocha && node generate-coverage-reports",
    "clean": "gulp clean",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bingnz/gulp-less-changed.git"
  },
  "keywords": [
    "gulpplugin",
    "less",
    "changed",
    "dependencies"
  ],
  "author": "David Chandler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bingnz/gulp-less-changed/issues"
  },
  "homepage": "https://github.com/bingnz/gulp-less-changed#readme",
  "devDependencies": {
    "@types/bluebird-global": "^3.5.4",
    "@types/less": "^0.0.32",
    "@types/mkdirp": "^0.5.2",
    "@types/plugin-error": "^0.1.0",
    "@types/replace-ext": "0.0.27",
    "@types/through2": "^2.0.33",
    "@types/vinyl": "^2.0.2",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "del": "^3.0.0",
    "fake-fs": "^0.5.0",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.0.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-replace": "^0.6.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^4.0.1",
    "istanbul": "^0.4.5",
    "lazypipe": "^1.0.1",
    "less": "^2.6.0",
    "merge2": "^1.2.1",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "pmock": "^0.2.3",
    "proxyquire": "^1.8.0",
    "remap-istanbul": "^0.10.1",
    "semantic-release": "^12.4.1",
    "sinon": "^4.2.2",
    "sinon-chai": "^2.14.0",
    "stream-assert": "^2.0.3",
    "typescript": "^2.7.1"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "mkdirp": "^0.5.1",
    "object-assign": "^4.1.1",
    "plugin-error": "^1.0.1",
    "replace-ext": "^1.0.0",
    "stream-to-array": "^2.3.0",
    "through2": "^2.0.3",
    "vinyl": "^2.1.0"
  },
  "peerDependencies": {
    "less": "^2.6.0"
  },
  "nyc": {
    "check-coverage": true,
    "extension": [
      ".js"
    ],
    "include": [
      "release/**/*.js"
    ],
    "require": [
      "babel-core/register",
      "babel-polyfill"
    ],
    "reporter": [
      "json"
    ],
    "sourceMap": true,
    "report-dir": "coverage"
  }
}
